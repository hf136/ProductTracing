<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.edu.bit.linc.dao.IBarcode">
    <select id="getBarcode" resultType="cn.edu.bit.linc.pojo.Barcode">
        SELECT * from barcode
    </select>
    
    <select id="getBarcodeByProductId" parameterType="Integer" resultType="cn.edu.bit.linc.pojo.Barcode">
         select * from barcode where productid = #{productID}
    </select>
    
    <insert id="addBarcode" parameterType="cn.edu.bit.linc.pojo.Barcode">
        insert into Barcode (productid) values (#{productID})
    </insert>
    
</mapper>
